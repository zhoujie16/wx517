/*列表  */
.list-view{
   position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
  background-color: rgb(241, 236, 230);
}
.list-title{
  position: relative;
  height: 130rpx;
  line-height: 170rpx;
  text-align: center;
  background-color: black;
  color: white;
}

.list-view scroll-view{
  width:100%;
  height: calc(100% - 130rpx);
}
.list-view scroll-view image{
  width: 100%;
}
.back-btn{
  position: relative;

  height: 100%;
}
.list-view scroll-view image{
  width: 100%;
}
.back-btn{
  position: absolute;
  background-image: url(data:image/png;base64,);
  width: 130rpx;
  height: 130rpx;
  top: 0;
  left: 0;
  background-repeat: no-repeat;
  background-size: 100rpx 100rpx;
  background-position: 0rpx 30rpx;
}
.img-mask{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgb(241, 236, 230);
}